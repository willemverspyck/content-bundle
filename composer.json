{
    "name": "spyck/content-bundle",
    "description": "Content Bundle for Symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Willem Verspyck",
            "email": "willemverspyck@outlook.com",
            "homepage": "https://verspyck.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "content",
        "php"
    ],
    "homepage": "https://github.com/willemverspyck/content-bundle",
    "type": "library",
    "require": {
        "php": "^8.1",
        "symfony/config": "^6|^7",
        "symfony/dependency-injection": "^6|^7",
        "symfony/framework-bundle": "^6|^7",
        "symfony/http-kernel": "^6|^7"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3",
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Spyck\\ContentBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spyck\\ContentBundle\\Tests\\": "tests/"
        }
    }
}
